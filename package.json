{
  "name": "raeuberbude",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.cjs",
    "start:dev": "npx concurrently \"ng serve --proxy-config proxy.conf.cjs\" \"npm --prefix backend run start\" \"npm --prefix backend/nest-app run start:dev\"",
    "start:network": "npx concurrently \"npm --prefix backend run start\" \"npm --prefix backend/nest-app run start:dev\" \"npm --prefix .specify/mcp-servers run all\" \"npx ng serve --host=0.0.0.0 --port=4301 --configuration=network --proxy-config proxy.conf.cjs\"",
    "start:network:seq": "scripts/start-network-sequential.bat",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:orange-light": "playwright test orange-light",
    "test:report": "playwright show-report test-results/playwright-report",
    "test:auto": "node .specify/scripts/auto-test-feature.js",
    "mcp:youtrack:local": "node .specify/mcp-servers/youtrack-mcp-server.js",
    "mcp:web:local": "node .specify/mcp-servers/web-mcp-server.js",
    "mcp:web:headed:win": "set MCP_HEADLESS=false&& node .specify/mcp-servers/web-mcp-server.js",
    "mcp:docker:local": "node .specify/mcp-servers/docker-mcp-server.js",
    "ha:dump": "node .specify/scripts/ha_dump.js",
    "prepare": "husky install",
    "serve:logs": "node server/index.js",
    "serve:cert": "node scripts/serve-cert.js",
    "health": "node scripts/health-check.js",
    "check-ports": "scripts/check-ports.bat",
    "restart": "npm stop --if-present && npm start",
    "start-all": "scripts/start-all-servers.bat"
  },
  "private": true,
  "dependencies": {
